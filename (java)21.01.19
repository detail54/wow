// 생성자

class Music {
	String title;
	int price;

	Music(String t, int p) {	//클래스명과 똑같이. 리턴이없기 때문에 void 등 반환형 작성 x
		title = t;				//메소드 매개변수를 클래스 변수에 대입
		price = p;
	}

	void show() {
		System.out.println(title + " " + price);		//클래스 매개변수를 출력하면 Music 생성자의 매개변수를 받아 출력된다.
	}

}

public class BB {

	public static void main(String[] args) {

		Music m = new Music("VVS",30000);	//생성자는 괄호안에 객체 생성.
		//m.title = "VVS";
		//m.price = 30000;		//변수 하나하나 설정하기 번거롭기 때문에 클래스 생성자를 불러와서 매개변수 바로 입력.
		
		m.show();

	}

}




